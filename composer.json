{
    "name": "veebisait/cross-alibaba",
    "description": "Veebisait/CrossAlibaba",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin",
            "email": "wb_zhang@sina.cn"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cross\\": "veebisait/cross-alibaba/src/"
        },
        "classmap": [
            "veebisait/cross-alibaba/src/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cross\\AlibabaProvider"
            ],
            "aliases": {
                "Alibaba": "Cross\\Facades\\Alibaba"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    }
}
